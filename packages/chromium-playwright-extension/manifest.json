{
  "manifest_version": 3,
  "name": "Playwright Locator Picker",
  "version": "0.1",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {},
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "scripting"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}